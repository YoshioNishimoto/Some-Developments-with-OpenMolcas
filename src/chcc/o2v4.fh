c
c        include for routines evaluating o2v4 contributions
c
c1.1        maximal # of Groups, SubGroups - in chcc1.fh
c
c1.2        Groups borders (which subgroup is low and upper limit of
c                           given group)
        integer Grpalow(1:maxGrp)
        integer Grpbelow(1:maxGrp)
        integer Grpaup(1:maxGrp)
        integer Grpbeup(1:maxGrp)
