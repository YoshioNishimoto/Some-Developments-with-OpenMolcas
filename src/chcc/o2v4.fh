c1.3        Dimensions of groups
        integer DimGrpa(1:maxGrp)
        integer DimGrpbe(1:maxGrp)
c
c1.4        Upper index lilits of groups
        integer UpGrpa(1:maxGrp)
        integer UpGrpbe(1:maxGrp)
c
c1.5        Lower index lilits of groups
        integer LowGrpa(1:maxGrp)
        integer LowGrpbe(1:maxGrp)
c
c1.6        Dimensions of subgroups
        integer DimSGrpa(1:maxSGrp)
        integer DimSGrpbe(1:maxSGrp)
c
c1.7        Upper index lilits of subgroups
        integer UpSGrpa(1:maxSGrp)
        integer UpSGrpbe(1:maxSGrp)
c
c1.8        Lower index lilits of subgroups
        integer LowSGrpa(1:maxSGrp)
        integer LowSGrpbe(1:maxSGrp)
c
c
        common /o2v4c1/ Grpalow,Grpbelow,Grpaup,Grpbeup,
     c                  DimGrpa,DimGrpbe,
     c                  UpGrpa,UpGrpbe,
     c                  LowGrpa,LowGrpbe,
     c                  DimSGrpa,DimSGrpbe,
     c                  UpSGrpa,UpSGrpbe,
     c                  LowSGrpa,LowSGrpbe
