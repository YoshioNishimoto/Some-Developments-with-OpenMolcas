*     This defines the highest angular momentum quantum number which
*     Seward will be able to treat.
#include "itmax.fh"
      Character AngTp(0:iTabMx)
#ifndef _DEMO_
      Data Angtp/'s','p','d','f','g',
     &           'h','i','k','l','m',
     &           'n','o','q','r','t',
     &           'u'/
#else
      Data Angtp/'s','p','d'/
#endif
