>> RM -FORCE TEST_HDF5
>> IF ( $MOLCAS_DRIVER = UNKNOWN_VARIABLE )
>>   EXPORT MOLCAS_DRIVER=molcas
>> ENDIF
>> SHELL $MOLCAS_DRIVER have_feature hdf5 || touch TEST_HDF5
>> IF ( -FILE TEST_HDF5 )
>>   EXIT 36
>> ENDIF

>> EXPORT MOLCAS_THR=2

&SEWARD
  Symmetry = x y
  Cholesky
  ThrCholesky = 1e-5
  Basis = N.3-21G.
    N1  0.000000  1.079353  0.212190  angstrom
  End of Basis
  Basis = C.3-21G.
    C2  0.000000  0.000000  0.997825  angstrom
    C5  0.000000  0.720633 -1.231924  angstrom
  End of Basis
  Basis = O.3-21G.
    O1  0.000000  2.299438  0.631108  angstrom
  End of Basis
  Basis = H.3-21G.
    H5  0.986863  0.999879 -1.589021  angstrom
    H2  0.000000  0.000000  2.083825  angstrom
  End of Basis

&SCF
  FileOrb = $Project.guessorb.h5
  ChoInput
    LocK
    dmpK = 1e-1
  EndChoInput
  UHF
  Occupied
   12 4 9 2
   12 3 9 2

&RASSCF
  FileOrb = $Project.scf.h5
  Symmetry = 2
  Spin = 2
  ChoInput
    LocK
    dmpK = 1e-1
  EndChoInput
  nActEl   = 7 0 0
  Inactive = 12  1  9  1
  Ras2     =  0  4  0  3
  CiRoot   = 2 2 1
  SDav = 100
  Iterations = 100 20
  CIMx = 100
  LevShift = 0.5

>> COPY $Project.rasscf.h5 JOB001

&RASSCF
  FileOrb = $Project.rasscf.h5
  Symmetry = 4
  Spin = 2
  ChoInput
    LocK
    dmpK = 1e-1
  EndChoInput
  nActEl = 7 0 0
  CiRoot = 3 3 1
  SDav = 100
  Iterations = 100 20
  CIMx = 100
  LevShift = 0.5

>> COPY $Project.rasscf.h5 JOB002

&RASSI
  NrOfJobIphs = 2 all
  CIPrint
  ChoInput
    Algo = 1
  EndChoInput

&RASSI
  NrOfJobIphs = 2 all
  CIPrint
  ChoInput
    LocK
    NoDecompose
    dmpk = 1e-1
  EndChoInput

