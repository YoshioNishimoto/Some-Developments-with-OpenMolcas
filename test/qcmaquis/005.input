>>>  Do  while  <<<
&SEWARD
>>> If ( ITER = 1 ) <<<
*SCF
>>>     EndIf    <<<
&DMRGSCF &END
ActiveSpaceOptimizer=QCMaquis
DMRGSettings
              nsweeps =   4
   max_bond_dimension =  50
    conv_thresh      = 1e-7
    truncation_final = 1e-8
    ietl_jcd_tol     = 1e-9
EndDMRGSettings
OOptimizationSettings
 inactive = 2 0 0 0 2 0 0 0
     RAS2 = 1 1 1 0 1 1 1 0
 CIROOT= 2 2; 1 2; 0 1
 RLXroot = 2
  SOCC
  2,2,2,0,0,0
  2,u,u,0,d,d
  LINEAR
  AVERAGE
  2; 2 3; 6 7
EndOOptimizationSettings
&ALASKA
&SLAPAF
